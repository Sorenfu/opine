<fieldset class='nested' data-nested-item='true'>
  <div class='field'>
    <%= f.label :text %>
    <%= f.text_area :text %>
  </div>
  <div class='field'>
    <%= f.label :alias %>
    <%= f.text_field :alias %>
  </div>
  <div class='field'>
    <%= f.label :graph_rgb %>
    <%= f.text_field :graph_rgb %>
  </div>
  <%= link_to_remove_field 'Remove this nested model', f %>
</fieldset>